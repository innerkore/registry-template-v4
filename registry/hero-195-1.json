      "content": "import * as React from \"react\";\nimport Link from \"@/components/Link\";\nimport { cn } from \"@/lib/utils\";\n\ninterface CardProps extends React.HTMLAttributes<HTMLDivElement> {\n    backgroundImage?: string;\n    backgroundColor?: string;\n}\n\nconst Card = React.forwardRef<HTMLDivElement, CardProps>(({ className, backgroundImage, backgroundColor, ...props }, ref) => (\n    <div\n        ref={ref}\n        className={cn(\n            \"rounded-lg border bg-card text-card-foreground shadow-sm\",\n            className,\n        )}\n        style={{ backgroundImage: backgroundImage ? `url(${backgroundImage})` : undefined, backgroundColor }}\n        {...props}\n    />\n));\nCard.displayName = \"Card\";\n\ninterface CardHeaderProps extends React.HTMLAttributes<HTMLDivElement> {}\n\nconst CardHeader = React.forwardRef<HTMLDivElement, CardHeaderProps>(({ className, ...props }, ref) => (\n    <div\n        ref={ref}\n        className={cn(\"flex flex-col space-y-1.5 p-6\", className)}\n        {...props}\n    />\n));\nCardHeader.displayName = \"CardHeader\";\n\ninterface CardTitleProps extends React.HTMLAttributes<HTMLHeadingElement> {\n    title: string;\n}\n\nconst CardTitle = React.forwardRef<HTMLHeadingElement, CardTitleProps>(({ title, className, ...props }, ref) => (\n    <h3\n        ref={ref}\n        className={cn(\n            \"text-2xl font-semibold leading-none tracking-tight\",\n            className,\n        )}\n        {...props}\n    >\n        {title}\n    </h3>\n));\nCardTitle.displayName = \"CardTitle\";\n\ninterface CardDescriptionProps extends React.HTMLAttributes<HTMLParagraphElement> {\n    description: string;\n}\n\nconst CardDescription = React.forwardRef<HTMLParagraphElement, CardDescriptionProps>(({ description, className, ...props }, ref) => (\n    <p\n        ref={ref}\n        className={cn(\"text-sm text-muted-foreground\", className)}\n        {...props}\n    >\n        {description}\n    </p>\n));\nCardDescription.displayName = \"CardDescription\";\n\ninterface CardContentProps extends React.HTMLAttributes<HTMLDivElement> {}\n\nconst CardContent = React.forwardRef<HTMLDivElement, CardContentProps>(({ className, ...props }, ref) => (\n    <div ref={ref} className={cn(\"p-6 pt-0\", className)} {...props} />\n));\nCardContent.displayName = \"CardContent\";\n\ninterface CardFooterProps extends React.HTMLAttributes<HTMLDivElement> {\n    buttonLabel: string;\n    buttonLink: string;\n}\n\nconst CardFooter = React.forwardRef<HTMLDivElement, CardFooterProps>(({ buttonLabel, buttonLink, className, ...props }, ref) => (\n    <div\n        ref={ref}\n        className={cn(\"flex items-center p-6 pt-0\", className)}\n        {...props}\n    >\n        <Link href={buttonLink} className=\"inline-flex items-center\">\n            {buttonLabel}\n        </Link>\n    </div>\n));\nCardFooter.displayName = \"CardFooter\";\n\nexport { Card, CardHeader, CardFooter, CardTitle, CardDescription, CardContent };"
    }
